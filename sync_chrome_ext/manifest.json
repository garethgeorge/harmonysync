{
  "name": "HarmonySync",
  "version": "1.0.0",
  "description": "an extension for syncing video playback on popular Video On Demand streaming sites",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["./build/bundle/background_script_bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./src/menu/menu.html"
  }
}
